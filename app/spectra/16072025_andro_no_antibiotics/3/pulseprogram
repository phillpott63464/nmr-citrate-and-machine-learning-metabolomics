# 1 "/opt/topspin3.6.1/exp/stan/nmr/lists/pp/user/zgprwgcpmg_mjh.k"
;zgprcpmg_mjh.gw
;1D sequence with presaturation and CPMG filter

"d11=30m"
"d12=20u"
"d13=3u"
"d20=1m"

"p7= 3*p1/13"
"p8= 9*p1/13"
"p9= 19*p1/13"
"d16=250u"
"d5=280u"
"d22=(d20+p1+p1)*l1"
# 1 "/opt/topspin3.6.1/exp/stan/nmr/lists/pp/Avance.incl" 1
